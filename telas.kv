<Inicio>:
    FloatLayout:

        canvas.before:
            Color:
                rgba: 0.678, 0.847, 1, 1  # Azul claro
            Rectangle:
                size: self.size
                pos: self.pos
        MDIconButton:
            on_press: app.mudar_tela('login')
            icon: 'logout'
            icon_size: '32sp'
            text: 'Sair'
            theme_text_color: "Custom"
            text_color: 'white'
            pos_hint: {'center_x': .1, 'center_y': .95}
        MDLabel:
            text: "Sair"
            theme_text_color: "Custom"
            font_name: "./fonts/Parkinsans-Medium.ttf"
            font_size: "14sp"
            text_color: 'white'
            halign: "center"
            valign: "middle"
            pos_hint: {'center_x': .09, 'center_y': .92}

        BoxLayout:
            orientation: 'vertical'
            size_hint_y: None
            height: "100dp"
            spacing: '20dp'
            pos_hint: {'center_x': .5, 'center_y': .85}
            Image:
                source: '.\imagens\logo.png'
                size_hint: None, None
                size: "150dp", "75dp"
                pos_hint: {'center_x': .5}

            MDLabel:
                text: "Bem-vindo!"
                theme_text_color: "Custom"
                font_name: "./fonts/Parkinsans-Medium.ttf"
                font_size: "24sp"
                text_color: 0.027, 0.263, 0.533, 1
                halign: "center"
                valign: "middle"
                pos_hint: {'center_x': .5}

        MDGridLayout:
            cols: 2
            size_hint_y: .6
            size_hint_x: .85
            pos_hint: {'center_x': .5, 'center_y': .4}
            padding:[dp(20), dp(20), dp(20), dp(35)]
            spacing: dp(25)
            MDCard:
                size_hint: .2, .2
                ripple_behavior: True
                pos_hint: {"center_x": .5, "center_y": .5}
                md_bg_color: 0.027, 0.263, 0.533, 1
                elevation: 1
                on_press: app.mudar_tela('associacao')
                MDBoxLayout:
                    orientation: 'vertical'
                    Image:
                        source: '.\imagens\cat.png'
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        size_hint_x: .6
                        size_hint_y: .6

                    MDLabel:
                        pos_hint: {'center_x': .5}
                        text: "Conheça os animais"
                        font_name: "./fonts/Parkinsans-Regular.ttf"
                        font_size: "17sp"
                        theme_text_color: "Custom"
                        text_color: 'white'
                        size_hint_x: 1
                        size_hint_y: .4
                        valign: "middle"
                        halign: 'center'
            MDCard:
                size_hint: .2, .2
                ripple_behavior: True
                pos_hint: {"center_x": .5, "center_y": .5}
                md_bg_color: 0.027, 0.263, 0.533, 1
                elevation: 1
                on_press: app.mudar_tela('voz')
                MDBoxLayout:
                    orientation: 'vertical'
                    Image:
                        source: '.\imagens\microphone.png'
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        size_hint_x: .6
                        size_hint_y: .6

                    MDLabel:
                        pos_hint: {'center_x': .5}
                        text: "Desenvolva a comunicação"
                        font_name: "./fonts/Parkinsans-Regular.ttf"
                        font_size: "17sp"
                        theme_text_color: "Custom"
                        text_color: 'white'
                        size_hint_x: 1
                        size_hint_y: .4
                        valign: "middle"  # Alinhando o texto no centro verticalmente
                        halign: 'center'
            MDCard:
                size_hint: .2, .2
                ripple_behavior: True
                pos_hint: {"center_x": .5, "center_y": .5}
                md_bg_color: 0.027, 0.263, 0.533, 1
                elevation: 1
                on_press: app.mudar_tela('cord_moto')
