                pos: self.pos
        MDTopAppBar:
            title: ""
            left_action_items: [["arrow-left", lambda x: app.mudar_tela('inicio')]]
            md_bg_color: 0.678, 0.847, 1, 1
            elevation: 0
        MDBoxLayout:
            orientation: 'vertical'
            size_hint_y: .5
