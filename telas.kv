ScreenManager:
    id: screen_manager
    Login:
        name:'login'
        id: login
    Register:
        name:'register'
        id: register
    Inicio:
        name: 'inicio'
        id: inicio
    Voz:
        name: 'voz'
        id: voz
    Associacao:
        name: 'associacao'
        id: associacao
    Associacao_jogo:
        name: 'asso_jogo'
        id: asso_jogo
    Emocoes:
        name: 'emocoes'
        id: emocoes
    Coordenacao_motora:
        name: 'cord_moto'
        id: cord_moto

<Login>:
    canvas.before:
        Color:
            rgba: 0.678, 0.847, 1, 1  # Azul claro
        Rectangle:
            size: self.size
            pos: self.pos
    Image:
        source: '.\imagens\logo.png'
        pos_hint: {'center_x': .5, 'center_y': .75}
        size_hint_x: .5

    MDTextField:
        id: user
        hint_text: 'Usuario:'
        foreground_color: 0, 1, 0, 1  # Verde
        text_color_focus: "black"
        line_color_focus: "black"
        hint_text_color_focus: "black"
        icon_left_color_normal: "black"
        icon_left_color_focus: "black"
        font_name: "./fonts/Parkinsans-Regular.ttf"
        pos_hint: {'center_x': .5, 'center_y': .6}
        font_name_hint_text: "./fonts/Parkinsans-Regular.ttf"
        size_hint_x: 0.7
        icon_left: "account"
        multiline: False

    MDTextField:
        id: password
        hint_text: 'Senha:'
        line_color_focus: "black"
        text_color_focus: "black"
        hint_text_color_focus: "black"
        icon_left_color_normal: "black"
        icon_left_color_focus: "black"
        font_name: "./fonts/Parkinsans-Regular.ttf"
        font_name_hint_text: "./fonts/Parkinsans-Regular.ttf"
        pos_hint: {'center_x': .5, 'center_y': .49}
        size_hint_x: 0.7
        icon_left: "key-variant"
        password: True
        multiline: False

    MDRaisedButton:
        id: entra
        text: 'Entrar'
        font_name: "./fonts/Parkinsans-Medium.ttf"
        pos_hint: {'center_x': .5, 'center_y': .375}
        size_hint_x: 0.7
        size_hint_y: .08
        elevation: 0
        on_press: root.logar()
        md_bg_color: 0.027, 0.263, 0.533, 1
        text_color: 1, 1, 1, 1


    MDTextButton:
        text: 'Novo por aqui? Cadastre-se'
        font_name: "./fonts/Parkinsans-Regular.ttf"
        pos_hint: {'center_x': .5, 'center_y': .3}
        size_hint_x: 0.7
        theme_text_color: 'Custom'
        text_color: 'black'
        font_size: "14sp"
        on_press: app.mudar_tela('register')

